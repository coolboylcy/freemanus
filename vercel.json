{
  "version": 2,
  "builds": [
    {
      "src": "web/backend/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web/backend/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "web/frontend/$1"
    }
  ]
} 